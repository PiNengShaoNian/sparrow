import test_utils for assert_equal

var a = []
for char ("sparrow") {
    a.add(char)
}

var sparrow = "sparrow"
assert_equal(a.count, sparrow.count, "`a.count == sparrow.count` assert failed!")
assert_equal(a.count, sparrow.count, "`a[0] == sparrow[0]` assert failed!")
assert_equal(a[0], sparrow[0], "`a[0] == sparrow[0]` assert failed!")
assert_equal(a[1], sparrow[1], "`a[1] == sparrow[1]` assert failed!")
assert_equal(a[2], sparrow[2], "`a[2] == sparrow[2]` assert failed!")
assert_equal(a[3], sparrow[3], "`a[3] == sparrow[3]` assert failed!")
assert_equal(a[4], sparrow[4], "`a[4] == sparrow[4]` assert failed!")
assert_equal(a[5], sparrow[5], "`a[5] == sparrow[5]` assert failed!")
assert_equal(a[6], sparrow[6], "`a[6] == sparrow[6]` assert failed!")
